
@model UdemyMVC5.ViewModel.RandomMovieViewModel.CustomersViewModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<table class="table table-hover">
    @foreach (var mov in Model.Customers)
    {
        <tr><td>@Html.ActionLink(mov.Name, "Detail", "Customers", new { id = mov.Id }, null)</td></tr>
    }
</table>